<?xml version="1.0" encoding="UTF-8"?>

<?import com.github.engatec.fxcontrols.FxTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox minHeight="-Infinity" minWidth="-Infinity" spacing="8.0" stylesheets="@../../styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.engatec.vdl.controller.stage.subscriptions.PlaylistContentsController">
   <children>
      <HBox alignment="BASELINE_CENTER" spacing="8.0">
         <children>
            <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" text="%subscriptions.name" />
            <FxTextField fx:id="subscriptionNameTextField" minWidth="-Infinity" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Pane prefHeight="20.0" VBox.vgrow="ALWAYS" />
      <ButtonBar buttonMinWidth="0.0" minHeight="-Infinity" minWidth="-Infinity">
        <buttons>
            <Button fx:id="subscribeBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" styleClass="control-btn" text="%subscribe" />
          <Button fx:id="closeBtn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" styleClass="control-btn" text="%close" />
        </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
